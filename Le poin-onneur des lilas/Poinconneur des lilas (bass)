  \bar "|."
}
 
 

 
 \book {
  \bookOutputSuffix "euphonium"
  \score {
    \new Staff \with {
      instrumentName = "Euphonium"
      midiInstrument = "tuba"
    } \bass
    \layout { }
    \midi {
      \tempo 4=140
    }
  }
}

trombone = {
   \transposition sib
  \transpose do do,
  \bass
  }
  
 \book {
  \bookOutputSuffix "trombone"
  
  \score {
    
    \new Staff \with {
      \clef bass
      instrumentName = "Trombone"
      midiInstrument = "trombone"
    } \trombone
    \layout { }
    \midi {
      \tempo 4=140
    }
  }
}



baryton = {
 \transposition mib
  \transpose mib' sib
  \bass
}

 \book {
  \bookOutputSuffix "baryton"
  \score {
    \new Staff \with {
      instrumentName = "Baryton"
      midiInstrument = "baritone sax"
    } \baryton
    \layout { }
    \midi {
      \tempo 4=140
    }
  }
}
