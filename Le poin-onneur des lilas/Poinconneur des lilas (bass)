\version "2.18.2"
\language "français"

\header {
  title = "Le poinçonneur des Lilas"
  subtitle = "Le Groupe électrogène Fanfare Club (from Pistons Circus)"
  composer = "Serge Gainsbourg"
}

global = {
  \key re \minor
  \time 4/4
  \tempo 4=120
}

